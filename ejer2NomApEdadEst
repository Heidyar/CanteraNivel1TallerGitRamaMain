Proceso  ejer2NomApEdadEst
	
	Definir nombre Como caracter; 
	Definir apellido Como caracter;
	Definir edad Como Entero;
	Definir estatura Como Real;
	
	Escribir "Nombre";
	Leer nombre;
	
	Escribir "Apellido";
	Leer apellido;
	
	Escribir "Edad";
	Leer edad;
	
	Escribir "Estatura";
	Leer estatura;
	
	Escribir  " Hola," ,nombre," ",apellido, ",tu edad es," ,edad, ",tu estatura es, ",estatura;
	   
FinAlgoritmo
